config ISP_HELLO
    bool "isp hello"
    default n
    ---help---
        Enable the api gpio example

if ISP_HELLO

config ISP_HELLO_PROGNAME
    string "Program name"
    default "isp_hello"

config ISP_HELLO_PRIORITY
    int "isp hello task priority"
    default 100

config ISP_HELLO_STACKSIZE
    int "isp hello stack size"
    default 2048

endif

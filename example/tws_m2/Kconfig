#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#

config EXAMPLES_TWS_M2
    tristate "tws m2 example"
    default y
    ---help---
        Enable the TWS M2 example

if EXAMPLES_TWS_M2

config EXAMPLES_TWS_M2_PROGNAME
    string "Program name"
    default "tws_m2"

config EXAMPLES_TWS_M2_PRIORITY
    int "tws m2 task priority"
    default 100

config EXAMPLES_TWS_M2_STACKSIZE
    int "tws m2 stack size"
    default 2048

endif

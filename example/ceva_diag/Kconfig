#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#

config EXAMPLES_CEVA_DIAG
    tristate "ceva diag example"
    default n
    ---help---
        Enable the ceva diag example

if EXAMPLES_CEVA_DIAG

config EXAMPLES_CEVA_DIAG_PROGNAME
    string "Program name"
    default "ceva_diag"

config EXAMPLES_CEVA_DIAG_PRIORITY
    int "ceva diag task priority"
    default 100

config EXAMPLES_CEVA_DIAG_STACKSIZE
    int "ceva diag stack size"
    default 2048

endif
